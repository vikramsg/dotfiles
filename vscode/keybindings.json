// Place your key bindings in this file to override the defaults
[
    {
        "key": "shift+cmd+g",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "editor.action.previousMatchFindAction",
        "when": "editorFocus"
    },
    {
        "key": "shift+cmd+g",
        "command": "-editor.action.previousMatchFindAction",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+g",
        "command": "workbench.action.terminal.findPrevious",
        "when": "terminalFindFocused && terminalHasBeenCreated || terminalFindFocused && terminalProcessSupported || terminalFocusInAny && terminalHasBeenCreated || terminalFocusInAny && terminalProcessSupported"
    },
    {
        "key": "shift+cmd+g",
        "command": "-workbench.action.terminal.findPrevious",
        "when": "terminalFindFocused && terminalHasBeenCreated || terminalFindFocused && terminalProcessSupported || terminalFocusInAny && terminalHasBeenCreated || terminalFocusInAny && terminalProcessSupported"
    },
    {
        "key": "ctrl+shift+g",
        "command": "workbench.action.terminal.openDetectedLink",
        "when": "accessibleViewIsShown && terminalHasBeenCreated && accessibleViewCurrentProviderId == 'terminal'"
    },
    {
        "key": "shift+cmd+g",
        "command": "-workbench.action.terminal.openDetectedLink",
        "when": "accessibleViewIsShown && terminalHasBeenCreated && accessibleViewCurrentProviderId == 'terminal'"
    },
    {
        "key": "ctrl+shift+l",
        "command": "aichat.close-sidebar"
    },
    {
        "key": "alt+cmd+g n",
        "command": "gitlens.toggleLineBlame"
    },
    {
        "key": "cmd+t cmd+d",
        "command": "editor.cpp.disableenabled"
    },
    {
        "key": "cmd+t cmd+e",
        "command": "editor.action.enableCppGlobally"
    },
    {
        "key": "cmd+i",
        "command": "composerMode.agent"
    },
    {
        "key": "ctrl+shift+t",
        "command": "editor.cpp.toggle"
    },
    {
        "key": "ctrl+b j",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b alt+down",
        "command": "-workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b k",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b alt+up",
        "command": "-workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b right",
        "command": "workbench.action.terminal.focus",
        "when": "accessibilityModeEnabled && accessibleViewOnLastLine && terminalHasBeenCreated && accessibleViewCurrentProviderId == 'terminal' || accessibilityModeEnabled && accessibleViewOnLastLine && terminalProcessSupported && accessibleViewCurrentProviderId == 'terminal'"
    },
    {
        "key": "cmd+down",
        "command": "-workbench.action.terminal.focus",
        "when": "accessibilityModeEnabled && accessibleViewOnLastLine && terminalHasBeenCreated && accessibleViewCurrentProviderId == 'terminal' || accessibilityModeEnabled && accessibleViewOnLastLine && terminalProcessSupported && accessibleViewCurrentProviderId == 'terminal'"
    },
    {
        "key": "ctrl+b left",
        "command": "workbench.action.focusActiveEditorGroup"
    },
    {
        "key": "ctrl+b right",
        "command": "terminal.focus"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.createTerminalEditor",
        "when": "terminalProcessSupported"
    },
    {
        "key": "ctrl+b h",
        "command": "workbench.action.terminal.resizePaneLeft",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b alt+left",
        "command": "-workbench.action.terminal.resizePaneLeft",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b l",
        "command": "workbench.action.terminal.resizePaneRight",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+b alt+right",
        "command": "-workbench.action.terminal.resizePaneRight",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+a d",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active && terminalFocus"
    },
    {
        "key": "ctrl+b d",
        "command": "-workbench.action.terminal.toggleTerminal",
        "when": "terminal.active && terminalFocus"
    },
    {
        "key": "ctrl+enter",
        "command": "workbench.action.terminal.sendSequence",
        "when": "terminalFocus",
        "args": {
            "text": "\\\r\n"
        }
    },
    {
        "key": "shift+enter",
        "command": "workbench.action.terminal.sendSequence",
        "when": "terminalFocus",
        "args": {
            "text": "\\\r\n"
        }
    },
    {
        "key": "cmd+[IntlBackslash]",
        "command": "claude-vscode.blur",
        "when": "!config.claudeCode.useTerminal && !editorTextFocus"
    },
    {
        "key": "cmd+escape",
        "command": "-claude-vscode.blur",
        "when": "!config.claudeCode.useTerminal && !editorTextFocus"
    },
    {
        "key": "shift+cmd+[IntlBackslash]",
        "command": "claude-vscode.terminal.open.keyboard",
        "when": "config.claudeCode.useTerminal"
    },
    {
        "key": "cmd+escape",
        "command": "-claude-vscode.terminal.open.keyboard",
        "when": "config.claudeCode.useTerminal"
    },
    {
        "key": "cmd+[IntlBackslash]",
        "command": "claude-vscode.focus",
        "when": "editorTextFocus && !config.claudeCode.useTerminal"
    },
    {
        "key": "cmd+escape",
        "command": "-claude-vscode.focus",
        "when": "editorTextFocus && !config.claudeCode.useTerminal"
    },
    {
        "key": "shift+cmd+[IntlBackslash]",
        "command": "claude-vscode.editor.open",
        "when": "!config.claudeCode.useTerminal"
    },
    {
        "key": "shift+cmd+escape",
        "command": "-claude-vscode.editor.open",
        "when": "!config.claudeCode.useTerminal"
    },
    {
        "key": "shift+cmd+x",
        "command": "workbench.view.extensions"
    }
]